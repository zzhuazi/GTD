<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"
        android:color="@android:color/holo_blue_light"></item> <!-- selected颜色 -->
    <item android:state_pressed="true"
        android:color="@android:color/holo_blue_dark"/> <!-- pressed颜色 -->
    <item android:state_focused="true"
        android:color="@color/colorAccent"/> <!-- focused颜色 -->
    <item android:color="@android:color/background_dark"/> <!-- default颜色 -->

</selector>